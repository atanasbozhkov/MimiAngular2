{
  "name": "marinastaneva",
  "version": "2.0.0",
  "description": "Marina Staneva's personal website rewritten in Angular.",
  "main": "server/app.js",
  "license": "All rights reserved.",
  "angular-cli": {},
  "scripts": {
    "start": "npm run _common:build | ng serve --host 127.0.0.1 --port 9000 --aot=false --proxy-config proxy.conf.json |npm run _server:run",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test --watch=false",
    "test-watch": "ng test --watch=true",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "npm run _common:build && ng build --prod --sm=false --aot=false --output-path=dist/client && npm run _server:build",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server",
    "_common:build": "tsc -p ./types",
    "postinstall": "npm run build"
  },
  "author": "Atanas Bozhkov",
  "engines": {
    "node": "8.9.4",
    "npm": "6.0.0"
  },
