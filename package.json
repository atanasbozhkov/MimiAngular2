{
  "name": "marinastaneva",
  "version": "2.0.0",
  "description": "Marina Staneva's personal website rewritten in Angular.",
  "main": "server/app.js",
  "license": "All rights reserved.",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --host 0.0.0.0 --aot=false --proxy-config proxy.conf.json |npm run _server:run",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test --watch=false",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build --prod --sm=false --aot=false --output-path=dist/client && npm run _server:build",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server",
    "postinstall": "npm run build"
  },
  "author": "Atanas Bozhkov",
  "engines": {
    "node": "8.9.1",
    "npm": "5.2.0"
  },
  "repository": {
    "url": "https://github.com/atanasbozhkov/MimiAngular2"
  },
  "private": true,
  "dependencies": {
